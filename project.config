[project]
	description = c++ course apps
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/oneacik
[access "refs/for/refs/*"]
	read = group user/oneacik plusplus
	push = group user/oneacik plusplus
	label-Code-Review = -2..+2 group user/oneacik plusplus
	label-Verified = -1..+1 group user/oneacik plusplus 
	rebase = group user/oneacik plusplus
	removeReviewer = group user/oneacik
	submit = group user/oneacik 
[access "refs/heads/*"]
	read = group user/oneacik
	create = group user/oneacik
	push = +force group user/oneacik
	pushMerge = group user/oneacik
[access "refs/tags/*"]
	push = group user/oneacik
	push = +force group user/oneacik
[receive]
	requireChangeId = true
